<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="car1" class="com.shinhan.mavenProject.section4.CarVO">
		<property name="model" value="ABC" />
		<property name="price" value="1000" />
		<property name="color" value="black" />
	</bean>

	<bean id="license1"
		class="com.shinhan.mavenProject.section4.LicenseVO">
		<property name="title" value="정보처리기사" />
		<property name="year" value="2020" />
	</bean>

	<bean id="license2"
		class="com.shinhan.mavenProject.section4.LicenseVO">
		<property name="title" value="SQLD" />
		<property name="year" value="2021" />
	</bean>

	<bean id="book1" class="com.shinhan.mavenProject.section4.BookVO">
		<constructor-arg name="title" value="이것이자바다" />
		<constructor-arg name="pirce" value="5000" />
		<constructor-arg name="kind" value="IT" />
	</bean>

	<bean id="book2" class="com.shinhan.mavenProject.section4.BookVO">
		<constructor-arg name="title" value="미시경제학" />
		<constructor-arg name="pirce" value="6000" />
		<constructor-arg name="kind" value="경제" />
	</bean>

	<bean id="person1" class="com.shinhan.mavenProject.section4.PersonVO">
		<property name="name" value="홍길동" />
		<property name="age" value="20" />
		<property name="car" ref="car1" />
		<property name="major">
			<list>
				<value>컴퓨터공학과</value>
				<value>경제학과</value>
			</list>
		</property>
		<property name="licenseList">
			<list>
				<ref bean="license1" />
				<ref bean="license2" />
			</list>
		</property>
		<property name="bookMap">
			<map>
				<entry>
					<key>
						<value>내 책1</value>
					</key>
					<ref bean="book1" />
				</entry>
				<entry>
					<key>
						<value>내 책2</value>
					</key>
					<ref bean="book2" />
				</entry>
			</map>
		</property>
		<property name="friends">
			<set>
				<value>전운종</value>
				<value>정진</value>
			</set>
		</property>
		<property name="myProfile">
			<props>
				<prop key="email">qkrtlgus2755@naver.com</prop>
				<prop key="phone">010-2316-0386</prop>
			</props>
		</property>
	</bean>

	<bean id="person2"
		class="com.shinhan.mavenProject.section4.PersonVO" autowire="byType"></bean>

</beans>
